{
  "name": "mxgraph-app",
  "workspaces": [
    "packages/*"
  ],
  "description": "MxGraph Diagram App",
  "repository": "https://github.com/kristianmandruo/mxgraph-app",
  "license": "MIT",
  "private": true,
  "scripts": {
    "commit": "npx git-cz",
    "l:version": "lerna version --conventional-commits --yes",
    "l:test": "lerna run test --since",
    "l:test:all": "lerna run test",
    "l:diff": "lerna diff",
    "l:test:toolbar": "lerna run test --scope=@mxgraph-app/toolbar",
    "l:test:menus": "lerna run test --scope=@mxgraph-app/menus",
    "prestart": "yarn build",
    "start": "run-p start:*",
    "start:stories": "lerna run start --stream --scope=@mxgraph-app/stories",
    "start:watch": "tsc --build --watch",
    "pretest": "yarn build",
    "test": "jest --config=jest.json",
    "test:stories": "lerna run test --stream --scope=@mxgraph-app/stories",
    "build": "tsc --build",
    "build:all": "lerna run build --stream --scope=@mxgraph-app/{dialogs,menus,mx,resources}",
    "build:stories": "lerna run build --stream --scope=@mxgraph-app/stories",
    "clean": "rimraf packages/*/{tsconfig.tsbuildinfo,lib,dist}"
  },
  "dependencies": {
    "jscolor": "^0.3.0",
    "Base64": "^1.1.0"
  },
  "devDependencies": {
    "commitizen": "^4.1.2",
    "@babel/core": "^7.9.6",
    "@babel/preset-env": "^7.9.6",
    "@babel/preset-react": "^7.9.4",
    "@types/jest": "^25.2.3",
    "@types/react": "^16.9.35",
    "@types/react-dom": "^16.9.8",
    "@types/react-test-renderer": "^16.9.2",
    "babel-jest": "^26.0.1",
    "jest": "^26.0.1",
    "lerna": "^3.21.0",
    "npm-run-all": "^4.1.5",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-test-renderer": "^16.13.1",
    "rimraf": "^3.0.2",
    "ts-jest": "^26.0.0",
    "typescript": "^3.9.3",
    "ts-mxgraph": "^1.0.0"
  }
}
